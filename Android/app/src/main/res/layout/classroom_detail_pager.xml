<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.example.richard_dt.visualisation.Activities.statActivity">

    <android.support.v7.widget.Toolbar android:id="@+id/my_toolbar6"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:tabTextColor="@color/tabUnselected"
        app:tabSelectedTextColor="@color/tabSelected"
        android:theme="@style/white"
        android:title="@string/TBTitle"
        app:logo="@mipmap/gslogo"
        android:background="?attr/colorPrimary"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" />
    <android.support.design.widget.TabLayout
        android:id="@+id/tabs"
        app:tabIndicatorColor="@color/jaunegs"
        app:colorPrimary="@color/colorPrimary"
        android:background="?attr/colorPrimary"
        app:tabTextColor="@color/tabUnselected"
        app:tabSelectedTextColor="@color/tabSelected"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/my_toolbar6"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/tabs">
        <android.support.v4.view.ViewPager
            android:id="@+id/vpPager3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >


        </android.support.v4.view.ViewPager>
    </FrameLayout>
</RelativeLayout>